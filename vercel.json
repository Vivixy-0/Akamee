{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "buildCommand": "next build",
  "ignoreCommand": "exit 0",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "eslint": {
          "ignoreDuringBuilds": true
        }
      }
    }
  ]
} 